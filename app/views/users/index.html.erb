
<div class='mainHeader'>Users</div><br>
<div><%= link_to 'Signup', new_user_path %></div><br>
<div><%= link_to 'View Current Rooms', rooms_path %></div><br>
<% @users.each do |u| %>
	<hr>
	<div><%= link_to u.username, u %></div><br>
	<div><%= u.first_name %> <%= u.last_name %></div><br>
	<div class='roomImage'><%= link_to(image_tag(u.avatar.url(:thumb)), u) %></div>
<% end %>

